<?xml version="1.0" encoding="UTF-8"?>
<form addfieldprefix="Joombooking\Component\Bookpro\Administrator\Field">
	<fields name="filter">
		<field
			name="search"
			type="text"
			inputmode="search"
			label="COM_BOOKPRO_FILTER_SEARCH_LABEL"
			
			hint="JSEARCH_FILTER"
		/>

		<field
			name=""
			type="type"
			label="Type"
			class="js-select-submit-on-change"
			>
			<option value="">COM_BOOKPRO_SELECT_TYPE</option>
		</field>	

		<field
			name=""
			type="brand"
			label="Brand"
			class="js-select-submit-on-change"
			>
			<option value="">COM_BOOKPRO_SELECT_BRAND</option>
		</field>

		<field
			name=""
			type="color"
			label="Color"
			class="js-select-submit-on-change"
			>
			<option value="">COM_BOOKPRO_SELECT_COLOR</option>
		</field>	
        	
	</fields>
	<fields name="list">
		
		<field
			name="limit"
			type="limitbox"
			label="JGLOBAL_LIST_LIMIT"
			default="25"
			class="js-select-submit-on-change"
		/>
	</fields>
</form>
